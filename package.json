{
    "name": "h5-server-open",
    "version": "1.0.0",
    "private": true,
    "description": "h5 server",
    "scripts": {
        "start": "node bin/www",
        "build-assets-dev": "cross-env NODE_ENV=dev webpack --config build-assets/webpack.dev.js",
        "build-assets-prd-dev": "cross-env NODE_ENV=prd_dev webpack --config build-assets/webpack.prd.js",
        "build-assets-prd": "cross-env NODE_ENV=production webpack --config build-assets/webpack.prd.js",
        "upload-assets-prd-dev": "cross-env NODE_ENV=prd_dev node build-assets/assets-upload-oss.js",
        "upload-assets-prd": "cross-env NODE_ENV=production node build-assets/assets-upload-oss.js",
        "dev": "npm run build-assets-dev && cross-env NODE_ENV=dev ./node_modules/.bin/nodemon bin/www",
        "prd-dev": "npm run build-assets-prd-dev && npm run upload-assets-prd-dev && cross-env NODE_ENV=prd_dev pm2 start bin/pm2-prd-dev.config.js",
        "prd": "npm run build-assets-prd && npm run upload-assets-prd && cross-env NODE_ENV=production NODE_LOG_DIR=/tmp/h5-server ENABLE_NODE_LOG=YES pm2 start bin/pm2-prd.config.js",
        "test:local": "cross-env NODE_ENV=test_local jest --runInBand  --passWithNoTests --colors --forceExit",
        "test:remote": "cross-env NODE_ENV=test_remote jest --runInBand  --passWithNoTests --colors --forceExit",
        "lint": "eslint \"src/**/*.{js,ts}\"",
        "lint-fix": "eslint --fix \"src/**/*.{js,ts}\"",
        "release": "release-it",
        "just-try-release": "release-it --dry-run"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/imooc-lego/h5-server.git"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/imooc-lego/h5-server/issues"
    },
    "homepage": "https://github.com/imooc-lego/h5-server#readme",
    "devDependencies": {
        "@babel/core": "^7.11.6",
        "@babel/preset-env": "^7.11.5",
        "@release-it/conventional-changelog": "^2.0.0",
        "babel-loader": "^8.1.0",
        "clean-webpack-plugin": "^3.0.0",
        "commitlint": "^11.0.0",
        "commitlint-config-cz": "^0.13.2",
        "copy-webpack-plugin": "^6.1.1",
        "css-loader": "^4.3.0",
        "cz-customizable": "^6.3.0",
        "eslint": "^7.8.1",
        "eslint-config-airbnb-base": "^14.2.0",
        "eslint-config-prettier": "^6.11.0",
        "eslint-plugin-import": "^2.22.0",
        "eslint-plugin-prettier": "^3.1.4",
        "husky": "^4.3.0",
        "jest": "^26.4.2",
        "lint-staged": "^10.3.0",
        "mini-css-extract-plugin": "^0.11.2",
        "nodemon": "^1.8.1",
        "optimize-css-assets-webpack-plugin": "^5.0.4",
        "prettier": "^2.1.1",
        "release-it": "^14.2.1",
        "style-loader": "^1.2.1",
        "supertest": "^6.1.3",
        "terser-webpack-plugin": "^4.2.2",
        "webpack": "^4.44.2",
        "webpack-cli": "^3.3.12",
        "webpack-merge": "^5.1.4"
    },
    "dependencies": {
        "@vue/server-renderer": "^3.0.0",
        "ali-oss": "^6.11.2",
        "axios": "^0.21.0",
        "cross-env": "^7.0.2",
        "debug": "^2.6.3",
        "fs-extra": "^9.0.1",
        "koa": "^2.2.0",
        "koa-bodyparser": "^3.2.0",
        "koa-convert": "^1.2.0",
        "koa-json": "^2.0.2",
        "koa-jwt": "^4.0.0",
        "koa-logger": "^2.0.1",
        "koa-onerror": "^1.2.1",
        "koa-router": "^7.1.1",
        "koa-static": "^3.0.0",
        "koa-views": "^5.2.1",
        "lego-components": "^0.1.4",
        "lodash": "^4.17.20",
        "mongoose": "^5.10.4",
        "mysql2": "^2.1.0",
        "pm2": "^4.4.1",
        "pug": "^2.0.0-rc.1",
        "redis": "^3.0.2",
        "require-all": "^3.0.0",
        "sequelize": "^6.3.5",
        "simple-git": "^2.20.1",
        "vue": "^3.0.0"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged",
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
        }
    },
    "lint-staged": {
        "*.js": [
            "npm run lint --fix",
            "npm run test:local"
        ]
    }
}
